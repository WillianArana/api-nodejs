    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^11.15.3",
    "@types/sequelize": "^4.28.8",
    "@types/validator": "^10.11.3",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
