{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "cyclomatic-complexity": [true, 12],
    "no-console": [false],
    "quotemark": [true, "single", "avoid-escape"],
    "object-literal-sort-keys": false,
    "max-line-length": [true, 120],
    "triple-equals": true,
    "semicolon": true,
    "ordered-imports": false,
    "no-trailing-whitespace": true,
    "interface-name": [false, "always-prefix"],
    "no-empty-interface": false,
    "no-unused-expression": true,
    "no-duplicate-variable": true,
    "no-empty": [true, "allow-empty-catch"],
    "typedef": [true, "call-signature", "property-declaration"],
    "max-classes-per-file": [true, 5, "exclude-class-expressions"],
    "no-magic-numbers": [false, 0, 1],
    "member-access": false,
    "member-ordering": [false, {"order": "fields-first"}],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "camelCase"
    ]
  },
  "trailing-comma": [true, {
    "singleline": "never",
    "multiline": {
      "objects": "always",
      "arrays": "always",
      "functions": "never",
      "typeLiterals": "ignore"
    }
  }],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rulesDirectory": []
}
