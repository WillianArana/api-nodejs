{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-console": [false],
    "quotemark": [true, "single", "avoid-escape"],
    "object-literal-sort-keys": false,
    "max-line-length": [true, 120],
    "triple-equals": true,
    "semicolon": true,
    "ordered-imports": false,
    "no-trailing-whitespace": true,
    "interface-name": [true, "always-prefix"],
    "no-empty-interface": false,
    "no-empty": false,
    "max-classes-per-file": [true, 5, "exclude-class-expressions"],
    "member-access": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "camelCase"
    ]
  },
  "trailing-comma": [
    true,
    {
      "singleline": "never",
      "multiline": {
        "objects": "ignore",
        "arrays": "always",
        "functions": "never",
        "typeLiterals": "ignore"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rulesDirectory": []
}
